{
	"$schema": "https://json.schemastore.org/prettierrc",
	"printWidth": 120,
	"semi": true,
	"tabWidth": 2,
	"useTabs": false,
	"trailingComma": "all",
	"singleQuote": false,
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"bracketSameLine": true,
	"htmlWhitespaceSensitivity": "ignore"
}
